#include <stdio.h>
#include "lcd.h"
#include "utility.h"
#include "drivers/mss_uart/mss_uart.h"


int main() {
	LCD_init();

	LCD_showRaceIntro();
	delay(6000);
	LCD_countdown();
	LCD_startTimer();
	delay(10000);
	LCD_showLeaderboard();
	LCD_showP1first();
	delay(10000);
	LCD_showP2first();
	delay(5000);
	LCD_showP1first();

	while(1) {

	}
}
